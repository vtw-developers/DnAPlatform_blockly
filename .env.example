# ========================================
# DnAPlatform 환경변수 설정
# ========================================
# 이 파일을 .env로 복사한 후 실제 값으로 교체하세요
# cp .env.example .env

# ========================================
# Database Configuration
# ========================================
DB_HOST=postgres
DB_PORT=5432
DB_NAME=blockly_db
DB_USER=blockly_user
DB_PASSWORD=blockly_password

# ========================================
# Backend Server Configuration
# ========================================
PORT=8000
SECRET_KEY=your_secret_key_here_change_this_in_production
ALLOWED_ORIGINS=http://localhost:5000,http://192.168.0.2:5050,http://121.65.128.115:5050

# ========================================
# Frontend Configuration
# ========================================
VITE_API_URL=http://192.168.0.2:8000/api
VITE_LOCAL_API_URL=http://192.168.0.2:8000/api
VITE_INTERNAL_API_URL=http://192.168.0.2:8000/api
VITE_PUBLIC_API_URL=http://121.65.128.115:8000/api
VITE_PORT=5000
VITE_INTERNAL_PORT=5050

# ========================================
# AI Service Configuration
# ========================================
OLLAMA_BASE_URL=http://192.168.0.2:11434
USE_LOCAL_LLM=true
ENABLE_OLLAMA=true
ENABLE_OPENAI=false

# OpenAI Configuration (Optional)
OPENAI_API_KEY=your_openai_api_key_here

# Available AI Models
AVAILABLE_MODELS='[
  {
    "name": "qwen2.5:72b",
    "type": "ollama",
    "description": "Qwen2.5 72B 모델"
  },
  {
    "name": "gpt-4o",
    "type": "openai",
    "description": "최신 GPT-4 모델, 코드 생성 능력 향상"
  }
]'

# ========================================
# Airflow Configuration
# ========================================
AIRFLOW_UID=50000
AIRFLOW_GID=0

# ========================================
# ZenML Configuration
# ========================================
ZENML_ANALYTICS_OPT_IN=false
ZENML_DEBUG=false
